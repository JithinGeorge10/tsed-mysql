    "@tsed/platform-response-filter": "^8.14.1",
    "@tsed/platform-views": "^8.14.1",
    "@tsed/schema": "^8.14.1",
    "@tsed/socketio": "^8.14.1",
    "@tsed/typegraphql": "^8.14.1",
    "ajv": "^8.17.1",
    "apollo-server-express": "2.25.2",
